{
  "cli": {
    "version": ">= 12.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "NPM_CONFIG_PACKAGE_LOCK": "false",
        "NPM_CONFIG_LOCKFILE": "false"
      },
      "cache": {
        "disabled": true
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "NPM_CONFIG_PACKAGE_LOCK": "false",
        "NPM_CONFIG_LOCKFILE": "false"
      },
      "cache": {
        "disabled": true
      }
    },
    "production": {
      "ios": {
        "simulator": false
      },
      "env": {
        "NPM_CONFIG_PACKAGE_LOCK": "false",
        "NPM_CONFIG_LOCKFILE": "false"
      },
      "cache": {
        "disabled": true
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "your-apple-id@example.com",
        "ascAppId": "your-app-store-connect-app-id",
        "appleTeamId": "your-apple-team-id"
      }
    }
  }
}